

    <body data-spy="scroll" data-target=".navbar" data-offset="30">

<section class="text-center">

    <div class="row">
      
    <hr>
    
      <div class="col-sm-8 mt-5">
        <h2 class="h2 text-info">moderator</h2>
      <input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search for names.." title="Type in a name">
      
      <% if(typeof data == 'undefined'){%> 
        <p>No User Register Yet</p>
          <%}else{%> 

      <table id="myTable" class="table table-responsive table-striped table-hover ml-2">
        <p class="p-2">
        <a href="/admin-dashboard">Admin Dashboard</a> |
        <a href="/all-blog">Edit Blog</a> |
        <a href="/post-blog">Create Post</a>
        </p>
        <h4>List of Subsribers</h4>
        <tr class="header">
         
          <th >Name</th>
          <th>Email</th>
          <th>Profile Status</th>
          <th>Status</th>  
          <th>View Prodfile</th>  
          <th>Register Date</th>  
           <th>SN</th> 
        </tr>

        <% count = data.length  %> 
        <% data.reverse().forEach((dt)=>{%>
      <% count -- %> 
        <tr>
          <td><%= dt.name  %> </td>
          <td><%= dt.email  %> </td>
          <td><%= dt.profileStatus  %> </td>

          <% if(dt.status == 'osaz@forum'){%>
            <td>Admin</td>
             <%}else{%>
          <td><%= dt.status  %> </td>
          <% } %> 

          <td>
           <button type="submit" style="border: none;"><a href="/view-a-profile/<%= dt.id  %> "><i class="fa fa-info"></i></a></button>
            </td> 
        
         <td><%= (dt.createdAt).toLocaleString()  %> </td>
         <td><%= count  %> </td>

        </tr>

        <% }) %> 
        
      </table>
      <% } %> 
    </div>
      
          <div class="col-sm-4 mt-5" style=" height:100vh;">
            <img src="/img/team-3.jpg" style="width: 100%;" alt="">
        </div>
    </div>

        
    </div>

</section>
<%- include('partials/footer.ejs') %> 